<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.az.md">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="扶뒬못"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ToolBarActivity"
            android:label="扶뒬못"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SearchActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SeachViewActivityTheme" />
        <activity
            android:name=".CardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CombinedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TabActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CollapsingActivity"
            android:screenOrientation="portrait" />

        <activity android:name=".CoordinatorActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".ImmersiveActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".StatusActivity"
            android:screenOrientation="portrait"
           />

        <activity android:name=".BottomSheetActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>